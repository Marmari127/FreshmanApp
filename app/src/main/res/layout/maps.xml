<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="@font/catamaran_medium"
        android:text="Location"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="match_parent"
        android:layout_height="182dp"
        android:layout_weight="0"
        app:srcCompat="@drawable/AJ3SO4H4PFEHJEBWWH65AAMYUM" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="@font/catamaran_medium"
        android:text="Directions"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_weight="0"
        android:fontFamily="@font/catamaran_medium"
        android:text="Coming from the north: Take highway 680 then take the exit onto 7th street."
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="@font/catamaran_medium"
        android:text="Coming from the south: Take highway 87 N and then the exit onto 280 S and then take the exit onto 7th street."
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="@font/catamaran_medium"
        android:text="Coming from the west: Take highway 280 S and then the exit onto 7th Street."
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="@font/catamaran_medium"
        android:text="Coming from the east: Take highway 280 N and then the exit onto 7th street."
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="@font/catamaran_medium"
        android:text="Parking"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="@font/catamaran_medium"
        android:text="North Garage: Located on San Fernando Street above the Student Success Center. "
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="@font/catamaran_medium"
        android:text="South Garage: Located on 7th and San Salvador Street."
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="@font/catamaran_medium"
        android:text="West Garage: Located on 4th and San Salvador Street."
        android:textColor="@android:color/black" />

</LinearLayout>
